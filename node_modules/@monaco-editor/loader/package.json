    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.14.0",
    "husky": "^9.1.6",
